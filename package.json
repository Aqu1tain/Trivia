{
  "name": "dailytrivia",
  "version": "2.1.0",
  "private": true,
  "description": "Bot Discord quotidien en français s’appuyant sur quizzapi.jomoreschi.fr pour diffuser des questions de culture générale et gérer les classements.",
  "main": "dist/bot/dailytrivia.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "dev": "ts-node-dev --respawn --transpile-only src/bot/dailytrivia.ts",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "deploy": "ts-node --project tsconfig.test.json scripts/deployer-commandes.ts",
    "clear:guild-commands": "ts-node --project tsconfig.test.json scripts/clear-guild-commands.ts"
  },
  "keywords": [
    "discord",
    "trivia",
    "francais"
  ],
  "author": "DailyTrivia Contributors",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "dayjs": "^1.11.18",
    "discord.js": "^14.22.1",
    "dotenv": "^17.2.2"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/nock": "^10.0.3",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "jest": "^30.1.3",
    "nock": "^14.0.10",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0"
  }
}
